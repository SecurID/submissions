{
  "name": "phonebook-backend",
  "version": "0.0.1",
  "description": "Backend for phonebook application",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
